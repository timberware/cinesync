{
  "name": "cinesync-api",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --maxWorkers=1",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config  ./test/jest-e2e.json --maxWorkers=1",
    "prepare": "cd ../ && husky install api/.husky",
    "db:generate": "npx --yes prisma generate --schema=prisma/schema.prisma",
    "db:migrate": "npx --yes prisma migrate dev",
    "db:push": "npx --yes prisma db push",
    "db:format": "npx --yes prisma format"
  },
  "dependencies": {
    "@keyv/redis": "^4.2.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.5",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@octokit/rest": "^21.1.1",
    "@prisma/client": "^5.3.0",
    "argon2": "^0.40.1",
    "axios": "^1.6.7",
    "cache-manager": "^6.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "keyv": "^5.2.3",
    "mailgun.js": "^9.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.5",
    "@octokit/types": "^13.10.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.3.64",
    "@types/cookie-parser": "^1.4.8",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.9",
    "@types/node": "^22.3.0",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "prisma": "^5.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
